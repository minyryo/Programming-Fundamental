#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n=100,mod=0,m=0;

    printf("Cac so nguyen to nho hon 100 :\n");
    //scanf("%d",&n);


        while (m<=n )
        {
            m=m+1;
            printf("m= %d :\n",m);
            int count=0;
            for (int i=1; i<= m; i++)
            {
                mod= m % i;
                if (mod == 0)
                count=count +1;
            }


            if (count>0 && count <=2)
                printf("%d la so nguyen to\n",m);
        }

    fflush(stdin);getchar();
    return 0;

}
